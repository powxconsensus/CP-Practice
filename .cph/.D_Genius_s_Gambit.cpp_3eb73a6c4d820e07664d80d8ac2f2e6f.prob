{"name":"D. Genius's Gambit","group":"Codeforces - Codeforces Round #704 (Div. 2)","url":"https://codeforces.com/contest/1492/problem/D","interactive":false,"memoryLimit":512,"timeLimit":2000,"tests":[{"input":"4 2 3","output":"Yes\n101000\n100001","id":1614152910992},{"input":"3 2 1","output":"Yes\n10100\n10010","id":1614152910939},{"input":"3 2 5\n","output":"No\n","id":1614152910943},{"id":1614154002477,"input":"#include <iostream>\n#include <algorithm>\n#include <vector>\n#include <cmath>\n#include <map>\n#include <set>\n#include <climits>\n\nusing namespace std;\n\n#define mod 998244353\n#define lli long long int\n#define ll long long\n#define li long int\n#define FastIO               \\\n    ios::sync_with_stdio(0); \\\n    cin.tie(0)\n\nclass PNC\n{\npublic:\n    vector<unsigned lli> f;\n    lli m;\n    PNC(lli n, lli m = mod)\n    {\n        f.resize(n + 1);\n        this->m = m;\n        f[0] = 1;\n        for (int i = 1; i <= n; i++)\n        {\n            f[i] = (i * f[i - 1]) % m;\n        }\n    }\n    lli power(lli b, lli p, lli m = mod)\n    {\n        lli res = 1;\n        while (p > 0)\n        {\n            if (p & 1)\n                res = ((res % m) * (b % m)) % m;\n            b = ((b % m) * (b % m)) % m;\n            p = p >> 1;\n        }\n        return res;\n    }\n    lli modInverse(lli n, lli m = mod) { return power(n, m - 2, m); }\n    lli factorial(lli n) { return f[n]; }\n    lli nCr(lli n, lli r)\n    {\n        if (r == 0)\n            return 1;\n        return (f[n] * modInverse(f[r], m) % m * modInverse(f[n - r], m) % m) % m;\n    }\n    lli nPr(lli n, lli r)\n    {\n        if (r == 0)\n            return 1;\n        return (f[n] * modInverse(f[n - r], m) % m) % m;\n    }\n};\n\nint main()\n{\n    // FastIO;\n    // ll a, b, c;\n    // cin >> a >> b >> c;\n    // if (a + b - 2 < c)\n    // {\n    //     cout << \"No\" << endl;\n    //     return 0;\n    // }\n    // string x, y;\n    // x.push_back('1');\n    // y.push_back('1');\n    // for (ll i = 0; i < (c + b - 1); i++)\n    //     x.push_back('1');\n    // for (ll i = 0; i < a; i++)\n    //     x.push_back('0');\n    // for (ll i = 0; i < b - 1; i++)\n    //     y.push_back('1');\n    // while (y.size() != x.size())\n    //     y.push_back('0');\n    // cout << \"Yes\" << endl;\n    // cout << x << endl;\n    // cout << y << endl;\n    FastIO;\n    ll tc;\n    cin >> tc;\n    while (tc--)\n    {\n        string str;\n        cin >> str;\n        // cout << str << endl;\n        ll n = str.size();\n        vector<vector<ll>> v(26);\n        for (ll i = 0; i < n; i++)\n        {\n            v[str[i] - 'a'].push_back(i);\n        }\n        // for (ll i = 0; i < 26; i++)\n        // {\n        //     for (ll j = 0; j < v[i].size() - 1; j++)\n        //     {\n        //         cout << v[i][j] << \" \";\n        //     }\n        //     cout << endl;\n        // }\n        ll ans = INT_MIN;\n        for (ll i = 0; i < 26; i++)\n        {\n            for (ll j = 0; j < (ll)v[i].size() - 1; j++)\n            {\n                ans = max(ans, abs(v[i][j] - v[i][j + 1]));\n                // cout << v[i][j] << \" \";\n            }\n        }\n        cout << ans - 1 << endl;\n    }\n\n    return 0;\n}","output":"1\n2"}],"testType":"single","input":{"type":"stdin"},"output":{"type":"stdout"},"languages":{"java":{"mainClass":"Main","taskClass":"DGeniussGambit"}},"srcPath":"c:\\Users\\mpriy\\Desktop\\My_WorkSpace\\CP-Practice\\D_Genius_s_Gambit.cpp"}